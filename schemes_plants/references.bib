@book{van_landuyt_monitoringsprotocol_2015,
title = "Monitoringsprotocol vaatplanten",
abstract = "Dit rapport beschrijft het protocol voor het monitoringmeetnet vaatplanten, mossen en korstmossen. We sommen de soorten op die gemonitord moeten worden en op welke manier dat moet gebeuren. Bij de hogere planten wordt voor een deel van de soorten het aantal individuen geteld, terwijl er voor andere oppervlaktebepalingen moeten gebeuren. We verduidelijken hoe voor de verschillende soorten het steekproefkader opgesteld werd. Aangezien het bij de planten gaat om relatief zeldzame soorten gebeurt de monitoring integraal. Per soort geven we vervolgens de lijst van te monitoren gebieden en de frequentie waarmee de tellingen moeten gebeuren. De mossen en korstmossen van de Bijlagen van de Europese Habitatrichtlijn worden opgevolgd in het kader van een reeds lopende monitoring door het INBO. Tenslotte verwijzen we naar het dataportaal waar de verzamelde gegevens ingevoerd moeten worden voor verdere analyse.",
author = "{Van Landuyt}, Wouter and Sam Provoost and Jo Packet and Dirk Maes and {De Bruyn}, Luc and {De Knijf}, Geert and Thierry Onkelinx and Frederic Piesschaert and Marc Pollet and {Van Calster}, Hans and Toon Westra and Paul Quataert",
note = "THIS MONITORING PROTOCOL HAS BEEN UPDATED. THE MOST RECENT VERSION IS AVAILABLE HERE: https://doi.org/10.21436/inbor.17097948",
year = "2015",
language = "Nederlands",
series = {Rapporten van het {Instituut} voor {Natuur}- en {Bosonderzoek}},
publisher = {Instituut voor Natuur- en Bosonderzoek},
address = {België},
number = "10039812",
}

